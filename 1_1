//Задание 1
#include <iostream>
using namespace std;

void decode(string a, int size)
{
    for (int j = 0; j < 3; j++) 
    {
        for (int i = size - 1; i > 0; i--)
        {
            char temp = a[i];
            a[i] = a[i - 1];
            a[i - 1] = temp;
        }
    }
    cout << "Output: " << a;
    
}
int main()
{
    cout << "Input: ";
    string s;
    cin >> s;
    int n = s.length();
    decode(s,n);
}
