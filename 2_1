//Задание 1
#include <iostream>
#include <string>
#include <fstream>
using namespace std;

void decode(string a, int size)
{
    for (int j = 0; j < 3; j++)
    {
        for (int i = size - 1; i > 0; i--)
        {
            char temp = a[i];
            a[i] = a[i - 1];
            a[i - 1] = temp;
        }
    }
    ofstream fout("output.txt");
    fout << a; // запись строки в файл
    fout.close(); // закрываем файл
    cout << a;
}
int F(int* a, int n, int sum, int i) {
    for (int i; i < n; i++) {
        F(a, n, sum, i);
    }
    return sum+
}
int main()
{
    setlocale(LC_ALL, "rus");
    string s;
    ifstream fin("input.txt");
    fin >> s;
    int n = stoi(s);
    int sum = 0;
    int a[26];
    for (int i = 0; i < n - 1; i++)
    {
        fin >> s;
        a[i] = stoi(s);
        
    }
    
    
}
